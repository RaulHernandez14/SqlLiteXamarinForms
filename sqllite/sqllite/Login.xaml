<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="sqllite.Login">
    
    <StackLayout Padding="15">
        <Entry Placeholder="Correo Electrónico" x:Name="CorreoEntry" />
        <Entry Placeholder="Contraseña" x:Name="ContraseñaEntry" IsPassword="True" />
        <Button Text="Iniciar Sesión" Clicked="IniciarSesion_Clicked" />
        <Button Text="Registarse" Clicked="Registrarse_Clicked" />
        
    </StackLayout>
</ContentPage>